  // ============================================================
  // SYNTAX HIGHLIGHTING - Cozy palette with warm tones
  // ============================================================
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7a6b67",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#c77f72",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#8ba87f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#c9a572"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#8a9db8"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#b58a9f"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#d4cbc7"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#b8845a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#c77f72"
      }
    },
    {
      "scope": [
        "support.constant",
        "constant.other"
      ],
      "settings": {
        "foreground": "#b8665a"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#a08f8b"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#c77f72",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#c9a572",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#b58a9f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#8ba87f"
      }
    },
    {
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#8a9db8",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#a84c41",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#b8665a",
        "fontStyle": "italic"
      }
    }
  ]
}
